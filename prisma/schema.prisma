generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id Int @id @default(autoincrement())
  displayName String @unique
  email String @unique
  picture String
  level Int 
  status String 
  wins Int 
  loses Int 
  role String 
  twoFactorAuthenticationSecret String 
  isTwoFactorAuthenticationEnabled Boolean 

}